# Create a vector of values
values <- 1:24  # 24 numbers

# Define dimensions (3x4x2)
dimensions <- c(3, 4, 2)

# Define dimension names
row_names <- c("Row1", "Row2", "Row3")
col_names <- c("Col1", "Col2", "Col3", "Col4")
matrix_names <- c("Matrix1", "Matrix2")

# Create the array
my_array <- array(values, dim = dimensions, dimnames = list(row_names, col_names, matrix_names))

# Print the array
print(my_array)
